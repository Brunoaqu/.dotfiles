    "nyc": "^15.1.0",
    "patch-package": "^7.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sinon": "^15.0.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
