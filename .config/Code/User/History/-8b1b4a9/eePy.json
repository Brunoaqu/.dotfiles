    "@opentelemetry/core": "^1.15.0",
    "@opentelemetry/exporter-jaeger": "^1.15.0",
    "@opentelemetry/instrumentation": "^0.41.0",
    "@opentelemetry/instrumentation-bunyan": "^0.32.0",
    "@opentelemetry/instrumentation-express": "0.28.0",
    "@opentelemetry/instrumentation-http": "^0.41.0",
    "@opentelemetry/instrumentation-knex": "^0.32.0",
    "@opentelemetry/instrumentation-mysql2": "^0.34.0",
    "@opentelemetry/instrumentation-redis": "^0.35.0",
    "@opentelemetry/instrumentation-winston": "^0.32.0",
    "@opentelemetry/resources": "^1.0.0",
    "@opentelemetry/sdk-trace-base": "^1.0.0",
    "@opentelemetry/sdk-trace-node": "^1.0.0",
    "@opentelemetry/semantic-conventions": "^1.0.0",
    "@opentelemetry/sql-common": "^0.40.0",
    "@redis/client": "^1.5.7",
    "ajv": "^8.11.2",
    "ajv-formats": "^2.1.1",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^11.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.5.1",
    "flatted": "^3.2.7",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "luxon": "^3.1.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.7.3",
    "nodemailer-smtp-transport": "^2.4.2",
    "path": "^0.12.7",
    "rand-token": "^1.0.1",
