    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.3.0",
    "@types/mime-types": "^2.1.2",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-smtp-transport": "^2.7.5",
    "@types/sinon": "^10.0.15",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
